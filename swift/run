#!/usr/bin/env zsh

PWD=$(cd "$(dirname "$0")";pwd)
source ${PWD}/../run_common.sh

LANGUAGE="swift"
EXECUTES=(
	hello
	basic
	character
	collection
	controlFlow
	closures
	enumeration
	classes
	subscript
	inheritan
	optionals
	errorHandle
	typeCasting
	nestedType
	protocols
	generics
	concurrency
)

if [[ ! -n $1 ]]; then
	read -p "swift run " name
else
	name=$1
fi

if [[ -n "$name" ]]; then
	echo "Run ${name}"
	swift run $name
fi
